{
  "name": "places-backend",
  "version": "1.0.0",
  "description": "Express backend server for places app.",
  "main": "server.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && yarn install && yarn build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.13",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.6.0"
  },
  "contributors": [
    "Sassan Shokoohi <sassansh@student.ubc.ca> (https://github.com/sassansh)",
    "Johnny Li <johnny@ualberta.ca> (https://github.com/johnnybcs)",
    "Amir Jafarvand <amir.jafarvand@gmail.com> (https://github.com/amirjfr)",
    "Laura Rodgers <laurarodgers@gmail.com> (https://github.com/laurarodgers)"
  ],
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  }
}
